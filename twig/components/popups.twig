<script>
    $('.m-profile-album-slider').owlCarousel({
        items: 3,
        slideBy: 3,
        loop: true,
        nav: true,
        dots: false,
        dotsEach: false,
        navText: ['', '3 фото <i class="icomoon icomoon-cheveron-right">\n' + ' \n' + ' </i>']
    });

    $('.js-m-male-photo-album-edit-slider').owlCarousel({
        items: 4,
        slideBy: 3,
        loop: true,
        nav: true,
        dots: false,
        dotsEach: false,
        navText: ['', '3 фото <i class="icomoon icomoon-cheveron-right">\n' + ' \n' + ' </i>']
    });

    $(function(){
        var owl = $('.js-m-male-photo-album-view-slider');
        owl.owlCarousel({
            items:1,
            dots: false,
            dotsEach: false,
            onInitialized  : counter,
            onTranslated : counter
        });

        function counter(event) {
            var element   = event.target;
            var items     = event.item.count;
            var item      = event.item.index + 1;
            $('#m-slide-counter').html(item + " из " + items);
        }
    });

    // open popup
    $('button').on('click', function () {
        var dataPopup = $(this).attr('data-call-popup');
        $(dataPopup).show();
    });

    // hide popup
    $('.js-m-btn-close-pop').on('click',function () {
        $('.mobile-popup').hide();
    });

    // chars counter
    $( function() {
        var maxLength = 500;
        $('textarea').keyup(function() {
            var length = $(this).val().length;
            var length = length;
            $('#chars').text(length);
        });
    });

    // sortable blocks
    $(function() {
        $("#m-top-droppable, #m-bottom-droppable").sortable({
            connectWith: ".m-album-page",
            addClasses: true
        }).disableSelection();
    });

//    profile text status
    $(".js-m-open-textarea").on("click", function () {
        $('.js-m-textarea-status-form ').addClass('js-m-textarea-status-form--active')
    });

    $(".js-m-textarea-status-save").on("click", function () {
        $('.js-m-textarea-status-form ').removeClass('js-m-textarea-status-form--active')
    });


</script>
